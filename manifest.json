{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "description": "Extension hides information that could promote personal bias",
  
  
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["personal-bias-blocker.css"],
    "js": ["personal-bias-blocker.js"]
  }],


  "browser_action": {
  	"default_title": "Personal Bias Blocker",
  	"default_popup": "popup.html"
  }
}